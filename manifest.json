{
  "manifest_version": 2,
  "version": "0.0.8.2",
  "name": "Negotiator",
  "author": "silentorbit",
  "short_name": "Negotiator",
  "minimum_chrome_version": "20.0.0.0",
  "minimum_edge_version": "33.14281.1000.0",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "description": "Surf the web on your terms",
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "19": "images/icon19.png",
      "38": "images/icon38.png",
      "128": "images/icon128.png"
    },
    "default_title": "Negotiator",
    "default_popup": "popup.html"
  },
  "update_url": "http://silentorbit.com/extensions/updates",
  "homepage_url": "http://silentorbit.com/negotiator/",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "incognito": "spanning",
  "inprivate": "spanning",
  "applications": {
    "gecko": {
      "id": "negotiator@silentorbit.com"
    }
  },
  "-ms-preload": {
    "backgroundScript": "backgroundScriptsAPIBridge.js",
    "contentScript": "contentScriptsAPIBridge.js"
  }
}